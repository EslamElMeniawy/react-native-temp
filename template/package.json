{
  "name": "TempApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "link-assets": "react-native-asset",
    "reverse": "adb start-server && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pods": "run-script-os",
    "pods:macos": "npx pod-install",
    "pods:default": "echo '\\033[0;33m\"pods\" command available on mac only!'",
    "android:dev": "react-native run-android --mode=developmentdebug",
    "android:staging": "react-native run-android --mode=stagingdebug",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:dev-release": "react-native run-android --mode=developmentrelease",
    "android:staging-release": "react-native run-android --mode=stagingrelease",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "apk:dev": "cd android && ./gradlew assembleDevelopment && cd ..",
    "apk:staging": "cd android && ./gradlew assembleStaging && cd ..",
    "apk:prod": "cd android && ./gradlew assembleProduction && cd ..",
    "bundle:dev": "cd android && ./gradlew bundleDevelopment && cd ..",
    "bundle:staging": "cd android && ./gradlew bundleStaging && cd ..",
    "bundle:prod": "cd android && ./gradlew bundleProduction && cd ..",
    "ios-clean": "run-script-os",
    "ios-clean:macos": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "ios-clean:default": "echo '\\033[0;33m\"ios-clean\" command available on mac only!'",
    "ios:dev": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Debug\"",
    "ios:dev-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Debug\"",
    "ios:dev-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Debug\"",
    "ios:dev-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Debug\"",
    "ios:staging": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Staging\"",
    "ios:staging-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Staging\"",
    "ios:staging-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Staging\"",
    "ios:staging-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Staging\"",
    "ios:prod": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Release\"",
    "ios:prod-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Release\"",
    "ios:prod-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Release\"",
    "ios:prod-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Release\"",
    "ios:dev-release": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Debug\" --configuration Release",
    "ios:staging-release": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Staging\" --configuration Release",
    "ios:prod-release": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --scheme \"Release\" --configuration Release",
    "start": "react-native start",
    "start-reset": "react-native start --reset-cache",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "postinstall": "patch-package && yarn pods && yarn link-assets && yarn android-clean && yarn ios-clean",
    "ver:test": "yarn version --prerelease --preid alpha && react-native-version --amend --never-increment-build",
    "ver": "yarn version && react-native-version --amend --reset-build",
    "svg": "react-native-vector-image generate",
    "reset": "react-native clean-project-auto && yarn"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-ft-flow": "^3.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-native-asset": "^2.1.1",
    "react-native-clean-project": "^4.0.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "run-script-os": "^1.1.6",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
