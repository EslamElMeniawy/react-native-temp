{
  "name": "TempApp",
  "version": "0.0.1-alpha.0",
  "private": true,
  "scripts": {
    "link-assets": "react-native-asset",
    "reverse": "adb start-server && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "android-clean": "rm -rf android/app/.cxx && cd android && ./gradlew clean && cd ..",
    "pods": "run-script-os",
    "pods:macos": "cd ios && bundle exec pod install --repo-update && cd ..",
    "pods:default": "echo '\\033[0;33m\"pods\" command available on mac only!'",
    "android:dev": "react-native run-android --mode=developmentdebug",
    "android:staging": "react-native run-android --mode=stagingdebug",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:dev-release": "react-native run-android --mode=developmentrelease",
    "android:staging-release": "react-native run-android --mode=stagingrelease",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "apk:dev": "cd android && ./gradlew assembleDevelopment && cd ..",
    "apk:staging": "cd android && ./gradlew assembleStaging && cd ..",
    "apk:prod": "cd android && ./gradlew assembleProduction && cd ..",
    "bundle:dev": "cd android && ./gradlew bundleDevelopment && cd ..",
    "bundle:staging": "cd android && ./gradlew bundleStaging && cd ..",
    "bundle:prod": "cd android && ./gradlew bundleProduction && cd ..",
    "ios-clean": "run-script-os",
    "ios-clean:macos": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "ios-clean:default": "echo '\\033[0;33m\"ios-clean\" command available on mac only!'",
    "ios:dev": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Debug\"",
    "ios:dev-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Debug\"",
    "ios:dev-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Debug\"",
    "ios:dev-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Debug\"",
    "ios:staging": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Staging\"",
    "ios:staging-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Staging\"",
    "ios:staging-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Staging\"",
    "ios:staging-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Staging\"",
    "ios:prod": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Release\"",
    "ios:prod-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Release\"",
    "ios:prod-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Release\"",
    "ios:prod-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Release\"",
    "ios:dev-release": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Debug\" --configuration Release",
    "ios:staging-release": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Staging\" --configuration Release",
    "ios:prod-release": "react-native run-ios --simulator=\"iPhone 16 Pro Max\" --scheme \"Release\" --configuration Release",
    "start": "react-native start",
    "start-reset": "react-native start --reset-cache",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepare": "lefthook install && bundle install",
    "postinstall": "patch-package && yarn pods && yarn link-assets && yarn android-clean",
    "ver:test": "yarn version ${0:-prerelease} && react-native-version --never-increment-build",
    "ver": "yarn version ${0} && react-native-version --reset-build",
    "svg": "react-native-vector-image generate",
    "reset": "react-native clean-project-auto && yarn",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.9.1",
    "@eslam-elmeniawy/react-native-common-components": "^1.4.0",
    "@klarna/react-native-vector-drawable": "^0.5.1",
    "@modules/assets": "link:./modules/assets",
    "@modules/components": "link:./modules/components",
    "@modules/core": "link:./modules/core",
    "@modules/localization": "link:./modules/localization",
    "@modules/theme": "link:./modules/theme",
    "@modules/utils": "link:./modules/utils",
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "^21.7.1",
    "@react-native-firebase/app": "^21.7.1",
    "@react-native-firebase/crashlytics": "^21.7.1",
    "@react-native-firebase/messaging": "^21.7.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@tanstack/query-sync-storage-persister": "^5.66.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-persist-client": "^5.66.0",
    "axios": "^1.7.9",
    "i18next": "^24.2.2",
    "react": "18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-native": "0.77.0",
    "react-native-bootsplash": "^6.3.3",
    "react-native-config": "^1.5.3",
    "react-native-device-info": "^14.0.2",
    "react-native-edge-to-edge": "^1.4.3",
    "react-native-gesture-handler": "^2.22.1",
    "react-native-keyboard-controller": "^1.16.1",
    "react-native-localize": "^3.4.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-network-logger": "^1.17.0",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-paper": "^5.13.1",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "^3.16.7",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^5.1.0",
    "react-native-screens": "^4.6.0",
    "react-native-shake": "^6.0.0-beta.3",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.11.1",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vector-image": "^0.4.5",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/runtime": "^7.26.7",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@evilmartians/lefthook": "^1.10.10",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@rnx-kit/align-deps": "^3.0.3",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@testing-library/react-native": "^13.0.1",
    "@types/i18n-js": "^4.0.1",
    "@types/react": "^19.0.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.0.0",
    "@types/redux-logger": "^3.0.13",
    "@types/tinycolor2": "^1.4.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-func": "^0.1.18",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "react-native-asset": "^2.1.1",
    "react-native-clean-project": "^4.0.3",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.12",
    "reactotron-react-native-mmkv": "^0.2.8",
    "reactotron-react-query": "^1.0.4",
    "run-script-os": "^1.1.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.6.0",
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.77"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "jest",
        "react",
        "react-test-renderer"
      ]
    }
  },
  "stableVersion": "0.0.1"
}
